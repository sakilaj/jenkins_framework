	<xs:simpleType name="Login">
		<xs:annotation>
			<xs:documentation>Id to identify the person / system who the request is coming from</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Password">
		<xs:annotation>
			<xs:documentation>User's Password</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BillingAccountNumber">
		<xs:annotation>
			<xs:documentation>Used as a base type for BAN</xs:documentation>
			<xs:documentation>Can be between 8,9, 12, or 13 digits</xs:documentation>
			<xs:documentation>Length: Max 13 as defined by the Atlas Schemas</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="13"/>
			<xs:pattern value="[0-9]{8}"/>
			<xs:pattern value="[0-9]{9}"/>
			<xs:pattern value="[0-9]{12}"/>
			<xs:pattern value="[0-9]{13}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EODGID">
		<xs:annotation>
			<xs:documentation>EOD_GROUP_ID field used to support the EOD Enhancements to Support Large Scale Solution</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="PhoneNumber">
		<xs:annotation>
			<xs:documentation>Used as a base type for Dialable number, MDN, MSISDN</xs:documentation>
			<xs:documentation>Exactly 11 digits</xs:documentation>
			<xs:documentation>Length: Min 11, Max 11</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="12"/>
			<xs:pattern value="[0-9]{11}"/>
			<xs:pattern value="[0-9]{12}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ContactPhoneNumber">
		<xs:annotation>
			<xs:documentation>Used as a base type for Dialable number, MDN, MSISDN</xs:documentation>
			<xs:documentation>1 or 50 digits</xs:documentation>
			<xs:documentation>Length: Min 1, Max 50</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Make">
		<xs:annotation>
			<xs:documentation>Device maker</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Model">
		<xs:annotation>
			<xs:documentation>Device model</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IMEI">
		<xs:annotation>
			<xs:documentation>Unique device identifier</xs:documentation>
			<xs:documentation>Max 15 digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="[0-9]{1,15}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MMSCapable">
		<xs:annotation>
			<xs:documentation>Boolean to identify whether a device is MMSCapable or not</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<xs:simpleType name="IMSI">
		<xs:annotation>
			<xs:documentation>SIM identifier</xs:documentation>
			<xs:documentation>Exactly 15 digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
			<xs:minLength value="14"/>
			<xs:pattern value="[0-9]{1,15}"/>
		</xs:restriction>
		<!--<xs:length value="15"/>-->
		<!--<xs:pattern value="[0-9]{15}"/>-->
	</xs:simpleType>
	<xs:simpleType name="ICCID">
		<xs:annotation>
			<xs:documentation>Unique SIM identifier</xs:documentation>
			<xs:documentation>Exactly 20 digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
			<xs:minLength value="18"/>
			<xs:pattern value="[0-9]{1,20}([0-9]{1})?"/>
		</xs:restriction>
		<!--<xs:pattern value="[0-9]{20}([0-9]{1})?"/>-->
	</xs:simpleType>
	<xs:simpleType name="WirelessServiceId">
		<xs:annotation>
			<xs:documentation>Unique customer identifier</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
