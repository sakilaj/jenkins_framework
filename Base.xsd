	<xs:complexType name="Product">
		<xs:annotation>
			<xs:documentation>This describes numerically what features are provisioned for any given subscriber. For example, having the 0021 product means the subscriber has email</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Attribute" type="Attribute" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="TransactionCode" type="TransactionCode" minOccurs="0"/>
			<xs:element name="System" type="System" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Category" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="255"/>
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Id" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="10"/>
					<xs:minLength value="4"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Action" type="Action" use="required"/>
		<xs:attribute name="ProductDescription" type="Description"/>
		<xs:attribute name="MajorCode" type="MajorCode"/>
		<xs:attribute name="Description" type="Description"/>
		<xs:attribute name="Status" type="SubscriberState"/>
		<xs:attribute name="EffectiveDate" type="xs:string" use="optional"/>
		<xs:attribute name="ExpirationDate" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Attribute">
		<xs:annotation>
			<xs:documentation>Attributes of a product that further describe or identify the behavior of a product. For example product eWallet's attribute describes the size of the wallet</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Name" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="50"/>
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="100"/>
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="MarketingOption">
		<xs:annotation>
			<xs:documentation>Name value sequence of various marketing options that the user can opt out of </xs:documentation>
		</xs:annotation>
		<xs:attribute name="Name" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="AWDoNotEmailPersonal"/>
					<xs:enumeration value="AWDoNotEmailBusiness"/>
					<xs:enumeration value="AWDoNotSMS"/>
					<xs:enumeration value="AWEmailSRUpdatesFlag"/>
					<xs:enumeration value="AWSuppressAllCalls"/>
					<xs:enumeration value="AWSuppressAllMailings"/>
					<xs:enumeration value="AWDoNotShareUsage"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Value" type="xs:boolean" use="required"/>
	</xs:complexType>
	<xs:complexType name="MarketingOptions">
		<xs:annotation>
			<xs:documentation>Parent of MarketingOption</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MarketingOption" type="MarketingOption" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TransactionCode">
		<xs:annotation>
			<xs:documentation>Contains the major code and description conveying the results of the operation</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionCodeList" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="TransactionCodeList"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="final" type="xs:boolean"/>
		<xs:attribute name="MajorCode" type="MajorCode" use="required"/>
		<xs:attribute name="Description" type="Description" use="required"/>
	</xs:complexType>
	<xs:complexType name="TransactionCodeList">
		<xs:annotation>
			<xs:documentation>Contains the minor code and description converying any intermediate steps taken in the operation</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ErrorCode" type="ErrorCode" use="required"/>
		<xs:attribute name="ErrorMessageText" type="ErrorMessageText" use="required"/>
		<xs:attribute name="System" type="SystemName"/>
		<xs:attribute name="TimeStamp" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="System">
		<xs:sequence>
			<xs:element name="DataItem" type="DataItem" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Products" type="Products" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Name" type="SystemName" use="required"/>
		<xs:attribute name="MajorCode" type="MajorCode"/>
		<xs:attribute name="Description" type="Description"/>
		<xs:attribute name="TimeStamp" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="DataItem">
		<xs:annotation>
			<xs:documentation>DataItems are the name/value pairs of data stored in systems</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Name" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="50"/>
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="100"/>
					<xs:minLength value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Header">
		<xs:sequence>
			<xs:element name="Sender" type="Sender" minOccurs="0"/>
			<xs:element name="Notification" type="Notification" minOccurs="0"/>
			<xs:element name="TransactionCode" type="TransactionCode" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="TransactionId" type="TransactionId" use="required"/>
		<xs:attribute name="ProvisioningCarrier" type="ProvisioningCarrier" use="required"/>
		<xs:attribute name="RoutingCarrier" type="RoutingCarrier"/>
		<xs:attribute name="TimeStamp" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="Sender">
		<xs:attribute name="Login" type="Login" use="required"/>
		<xs:attribute name="Password" type="Password" use="required"/>
	</xs:complexType>
	<xs:complexType name="Notification">
		<xs:attribute name="URL" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="BillingAccountAddress">
		<xs:annotation>
			<xs:documentation>Billing Account Address for use with AccountNotification Solution</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" type="xs:string"/>
		<xs:attribute name="AddressLine1" type="Street"/>
		<xs:attribute name="AddressLine2" type="Street"/>
		<xs:attribute name="City" type="City"/>
		<xs:attribute name="State">
			<xs:simpleType>
				<xs:restriction base="State">
					<xs:length value="2"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="PostalCode" type="PostalCode"/>
		<xs:attribute name="InternationalPostalCode" type="InternationalPostalCode"/>
		<xs:attribute name="InternationalAddressLine" type="InternationalAddressLine"/>
		<xs:attribute name="PostalPlusCode" type="PostalPlusCode"/>
		<xs:attribute name="CountryName" type="CountryName"/>
		<xs:attribute name="CountryCode" type="CountryCode"/>
		<xs:attribute name="LocalCompanyName" type="LocalCompanyName"/>
		<xs:attribute name="GeoCode" type="GeoCode"/>
		<xs:attribute name="CountyName" type="CountyName" use="optional"/>
	</xs:complexType>
	<xs:complexType name="BillingAccountName">
		<xs:annotation>
			<xs:documentation>Billing Account Name for use with AccountNotification Solution</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" type="xs:string"/>
		<xs:attribute name="BusinessName" type="xs:string"/>
		<xs:attribute name="DoingBusinessNameAs" type="xs:string"/>
		<xs:attribute name="FirstName" type="FirstName"/>
		<xs:attribute name="LastName" type="LastName"/>
	</xs:complexType>
	<xs:complexType name="BillingAccountPhone">
		<xs:annotation>
			<xs:documentation>Billing Account Phone for use with AccountNotification Solution</xs:documentation>
		</xs:annotation>
		<xs:attribute name="homePhone" type="xs:string"/>
		<xs:attribute name="workPhone" type="xs:string"/>
		<xs:attribute name="workPhoneExtension" type="xs:string"/>
		<xs:attribute name="canBeReachedPhone" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BillingAccountEmail">
		<xs:annotation>
			<xs:documentation>Billing Account Email for use with AccountNotification Solution</xs:documentation>
		</xs:annotation>
		<xs:attribute name="emailType" type="xs:string"/>
		<xs:attribute name="emailAddress" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BillingAccountType">
		<xs:sequence>
			<xs:element name="FAN" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="currentFAN" type="xs:string"/>
					<xs:attribute name="previousFAN" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="FANName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The profile name associated with the FAN, The legal name of the company to bill within billingInfo of CDM, can be extracted from IAP SOAP dip, maxlength is 60</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ContractId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>K40407 -added to support corporate copay functionality </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Each FAN must be associated with a contract type. e.g. CMSA. BSA, etc.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BillingCycle" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="current" type="xs:string"/>
					<xs:attribute name="previous" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Name" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="current" type="BillingAccountName" minOccurs="0"/>
						<xs:element name="previous" type="BillingAccountName" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Address" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="current" type="BillingAccountAddress" minOccurs="0"/>
						<xs:element name="previous" type="BillingAccountAddress" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Phone" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="current" type="BillingAccountPhone" minOccurs="0"/>
						<xs:element name="previous" type="BillingAccountPhone" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Email" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="current" type="BillingAccountEmail" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="previous" type="BillingAccountEmail" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BANEmail" minOccurs="0">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation>PID-259416- Billing Account Email -Added to support Billing Account Email request</xs:documentation>
					</xs:annotation>
					<xs:attribute name="BANEmailType" type="xs:string"/>
					<xs:attribute name="BANEmailAddress" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="BillingMarket" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="current" type="xs:string"/>
					<xs:attribute name="previous" type="xs:string"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FinancialNotificationType">
		<xs:attribute name="AdjustmentAmount" type="xs:string"/>
		<xs:attribute name="AdjustmentCode" type="xs:string"/>
	</xs:complexType>
	<!--For use with Femtocell Project Only -->
	<xs:complexType name="TNAddress">
		<xs:annotation>
			<xs:documentation>TNUpdate Address Structure</xs:documentation>
		</xs:annotation>
		<xs:attribute name="HNO" type="xs:string"/>
		<xs:attribute name="HNS" type="xs:string"/>
		<xs:attribute name="PRD" type="xs:string"/>
		<xs:attribute name="STN" type="xs:string"/>
		<xs:attribute name="MCN" type="xs:string"/>
		<xs:attribute name="STA" type="xs:string"/>
		<xs:attribute name="LOC" type="xs:string"/>
		<xs:attribute name="STS" type="xs:string"/>
		<xs:attribute name="POD" type="xs:string"/>
		<xs:attribute name="ZIP" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TNInfo">
		<xs:annotation>
			<xs:documentation>TNUpdate Key and Option Structure</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ExternalKey" type="xs:string"/>
		<xs:attribute name="NENAID" type="xs:string"/>
		<xs:attribute name="CLS" type="xs:string"/>
		<xs:attribute name="TYS" type="xs:string"/>
		<xs:attribute name="Latitude" type="xs:string"/>
		<xs:attribute name="Longitude" type="xs:string"/>
		<!--Added for 225914 3G Microcell Lat/Long BR for 1010 release-->
	</xs:complexType>
	<xs:complexType name="TNUpdateInfo">
		<xs:sequence>
			<xs:element name="FemtocellAddress" type="TNAddress" minOccurs="0"/>
			<xs:element name="FemtocellAdditionalInfo" type="TNInfo" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CancelFlag">
		<xs:annotation>
			<xs:documentation>Boolean to identify whether a the transaction is a Subscriber Cancel or not</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean"/>
		<!--For use if there is a need to notify of complete subscriber account cancellation.-->
		<!-- Currently being used with xDCP (aka RedSea / SAME) Project.-->
	</xs:simpleType>
	<xs:simpleType name="GUIDInfo">
		<xs:annotation>
			<xs:documentation>Used as a base type for SLID GUID and UVerse Member GUID</xs:documentation>
			<xs:documentation>Is 12 digits</xs:documentation>
			<xs:documentation>Length: Max 12 as defined by the Red Sea CSI and 3PP Microsoft Deliverables - Working Session on 10/18/10</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{1,12}"/>
		</xs:restriction>
		<!--BEGIN For use with xDCP (aka RedSea / SAME) Project Only -->
	</xs:simpleType>
	<!--This is the User 1 or 2 with UserID and associated Attached IDs for that user -->
	<xs:complexType name="SelectedUserInfo">
		<xs:choice>
			<xs:element name="AccessID" type="xs:string">
				<xs:annotation>
					<xs:documentation>Also known as the SLID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MemberID" type="xs:string">
				<xs:annotation>
					<xs:documentation>Also known as the StandAlone Uverse MemberID</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="MobilityInfo">
		<xs:attribute name="MSISDN" type="PhoneNumber" use="required"/>
		<xs:attribute name="SubscriberID" type="SubscriberNumber" use="required"/>
	</xs:complexType>
	<xs:complexType name="UserIDInfo">
		<xs:sequence>
			<xs:element name="ID" type="SelectedUserInfo"/>
			<xs:element name="GUID" type="GUIDInfo">
				<xs:annotation>
					<xs:documentation>Generated Unique ID</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AttachedIDInfo">
		<xs:choice>
			<xs:element name="MobilityID" type="MobilityInfo"/>
			<xs:element name="AttachedID" type="UserIDInfo"/>
		</xs:choice>
		<xs:attribute name="AttachAction" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Action to be taken on the AttachedUser.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MainUserID" type="xs:boolean" use="required">
			<xs:annotation>
				<xs:documentation>Attribute to denote that this AttachedID is the MainUserID of the account.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="UserInfo">
		<xs:sequence>
			<xs:element name="UserID" type="UserIDInfo">
				<xs:annotation>
					<xs:documentation>User Id Information.  This element will contain the User's GUID and the User's AccessID or User's MemberID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AttachedIDs" type="AttachedIDInfo" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Action" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Action to be taken on the User.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ImmutableKeyInfo">
		<xs:sequence>
			<xs:element name="User" type="UserInfo" maxOccurs="2"/>
		</xs:sequence>
	</xs:complexType>
	<!--END For use with xDCP (aka RedSea / SAME) Project Only -->
	<!--BEGIN CR 97806 - Toggle Phase 2 Bolt On 262131a -->
	<xs:complexType name="EventTypeInfo">
		<xs:annotation>
			<xs:documentation>For Transaction Event Name</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EventName">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--END For CR 97806 - Toggle Phase 2 Bolt On 262131a -->
	<!--BEGIN 248384 CR 67584 - Frontier Hybrid Billing Model -->
	<xs:complexType name="CombinedBillingType">
		<xs:sequence>
			<xs:element name="BillingTelephoneNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>BillingTelephoneNumber contains the 10 digit Telco BTN.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="RAO" minOccurs="0">
				<xs:annotation>
					<xs:documentation>RAO.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="HybridAgent" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hybrid Agent Identifier.  Referenced from CombinedBillingRegion.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CombinedBillingStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field provides the status of the Combined Billing.
						P - Pending Combine
						Y - Enrolled / Combined
						N - Not Enrolled / De-Enrolled / De-Combined,
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CombinedBillingEnrollmentDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field contains the timestamp of the message generation.  This is to be considered the Enrollment / De-Enrollment date of the Combined Billing.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--END 248384 CR 67584 - Frontier Hybrid Billing Model -->
	<!--BEGIN 269471 - Digital Life -->
	<xs:complexType name="TaxExemptionInfo">
		<xs:attribute name="status" type="xs:string"/>
		<xs:attribute name="entityType" type="xs:string"/>
		<xs:attribute name="effectiveDate" type="xs:string"/>
		<xs:attribute name="expirationDate" type="xs:string"/>
	</xs:complexType>
	<!--END 269471 - Digital Life -->
</xs:schema>
